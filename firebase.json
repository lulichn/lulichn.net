{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/slide/**",
        "destination": "https://lulichn-slides.firebaseapp.com/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
